<?xml version="1.0" encoding="UTF-8"?>

<screen require-authentication="anonymous-all"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-2.1.xsd">

    <transition name="json" method="options" require-session-token="false">
        <default-response type="none" />
    </transition>

    <pre-actions><script>
        // if user not already logged in redirect to login page
        if (!ec.user.userId) sri.sendRedirectAndStopRender('./MKLoginPage')
    </script></pre-actions>

    <widgets>
        <!-- <render-mode>
            <text type="html,vuet" location="component://MKSignUp/screen/MKSignUp/Pages/MKWelcomePage.html"/>
        </render-mode> -->
        <label type="h1" text="Thank you for signing up."/>
        <label type="p" text="We will be in contact with you shortly."/>
        <label type="p" text="-- MK Decision Team"/>
    </widgets>


</screen> 